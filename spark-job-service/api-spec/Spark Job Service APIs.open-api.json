  "components": {
    "schemas": {
      "DailySalesReportJobLaunchRequest": {
        "required": [
          "correlationId",
          "jobName",
          "month",
          "sparkConfigs"
        ],
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/JobLaunchRequest"
          },
          {
            "type": "object",
            "properties": {
              "month": {
                "type": "object",
                "properties": {
                  "year": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "month": {
                    "type": "string",
                    "enum": [
                      "JANUARY",
                      "FEBRUARY",
                      "MARCH",
                      "APRIL",
                      "MAY",
                      "JUNE",
                      "JULY",
                      "AUGUST",
                      "SEPTEMBER",
                      "OCTOBER",
                      "NOVEMBER",
                      "DECEMBER"
                    ]
                  },
                  "monthValue": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "leapYear": {
                    "type": "boolean"
                  }
                },
                "description": "Report for the month",
                "nullable": true,
                "example": "2024-11"
              }
            }
          }
        ]
      },
      "JobLaunchRequest": {
        "required": [
          "correlationId",
          "jobName",
          "sparkConfigs"
        ],
        "type": "object",
        "properties": {
          "jobName": {
            "type": "string",
            "description": "Spark Job name, must be present in application.yml spark-submit.jobs",
            "example": "daily-sales-report-job"
          },
          "correlationId": {
            "type": "string",
            "description": "Unique correlation id for each Job execution.",
            "example": "71643ba2-1177-4e10-a43b-a21177de1022"
          },
          "sparkConfigs": {
            "type": "object",
            "additionalProperties": {
              "type": "object",
              "description": "Runtime Spark conf properties for this job.",
              "example": {
                "spark.executor.instances": 4,
                "spark.driver.cores": 3
              }
            },
            "description": "Runtime Spark conf properties for this job.",
            "example": {
              "spark.executor.instances": 4,
              "spark.driver.cores": 3
            }
          }
        },
        "discriminator": {
          "propertyName": "jobName"
        }
      },
      "LogsAnalysisJobLaunchRequest": {
        "required": [
          "correlationId",
          "jobName",
          "sparkConfigs"
        ],
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/JobLaunchRequest"
          }
        ]
      },
      "SparkExampleJobLaunchRequest": {
        "required": [
          "correlationId",
          "jobName",
          "sparkConfigs"
        ],
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/JobLaunchRequest"
          }
        ]
      },
      "JobExecution": {
        "type": "object",
        "properties": {
          "jobName": {
            "type": "string"
          },
          "executionId": {
            "type": "integer",
            "format": "int64"
          },
          "correlationId": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "startTime": {
            "type": "string",
            "format": "date-time"
          },
          "endTime": {
            "type": "string",
            "format": "date-time"
          },
          "duration": {
            "type": "string"
          },
          "exitMessage": {
            "type": "string"
          },
          "errorMessage": {
            "type": "string"
          },
          "arguments": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "Links": {
        "type": "object",
        "additionalProperties": {
          "$ref": "#/components/schemas/Link"
        }
      },
      "PaginatedResourceJobExecution": {
        "type": "object",
        "properties": {
          "content": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/JobExecution"
            }
          },
          "page": {
            "$ref": "#/components/schemas/PaginationData"
          }
        }
      },
      "PaginationData": {
        "required": [
          "totalRecords"
        ],
        "type": "object",
        "properties": {
          "currentPage": {
            "type": "integer",
            "description": "Current page number",
            "format": "int32",
            "example": 3
          },
          "pageSize": {
            "type": "integer",
            "description": "Page size, number of records per page",
            "format": "int32",
            "example": 16
          },
          "totalPages": {
            "type": "integer",
            "description": "Total number of pages available, matching given filters",
            "format": "int32",
            "example": 35
          },
          "totalRecords": {
            "type": "integer",
            "description": "Total number of records available, matching given filters",
            "format": "int64",
            "example": 145
          },
          "_links": {
            "$ref": "#/components/schemas/Links"
          },
